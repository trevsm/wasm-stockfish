{
  "name": "wasm-stockfish",
  "private": true,
  "version": "1.2.1",
  "type": "module",
  "scripts": {
    "postinstall": "node -e \"const fs=require('fs');const p=require('path');const src=p.join('node_modules','stockfish','bin','stockfish-18-lite-single');const dest=p.join('public');fs.mkdirSync(dest,{recursive:true});fs.copyFileSync(src+'.js',p.join(dest,'stockfish.js'));fs.copyFileSync(src+'.wasm',p.join(dest,'stockfish.wasm'));\"",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "fetch-puzzles": "node scripts/fetch-puzzles.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "chess.js": "^1.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0",
    "stockfish": "^18.0.5",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.12.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.1",
    "vite-plugin-pwa": "^1.2.0"
  }
